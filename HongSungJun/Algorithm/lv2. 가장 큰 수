function solution(numbers) {
    numbers = numbers.map((el) => String(el))
   
    numbers.sort((a,b) => {
        if(a+b > b+a) return -1
        else if(a+b < b+a ) return 1
    })
    const answer = numbers.join("")
    
    if(answer[0] === "0"){
        return "0"
    }
    else{
    return answer
        }
   
    
    
}